{
  "version": "1.0.1",
  "license": "AGPL-3.0-or-later",
  "name": "@constl/relai-libp2p",
  "author": "Julien Jean Malard-Adam",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": "./dist/src/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "nettoyer": "rimraf dist",
    "compiler": "pnpm versioner && pnpm formatter && pnpm nettoyer && pnpm tspc -p tsconfig.json",
    "compiler:prod": "pnpm versioner && pnpm formatter && pnpm nettoyer && pnpm tspc -p tsconfig.build.json",
    "test": "pnpm compiler && pnpm aegir test -t node --cov -- --exit",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint",
    "versioner": "pnpm genversion  --es6 --semi src/version.ts",
    "lancer": "pnpm compiler:prod && node --env-file=.env dist/serveur.js",
    "effacer-pairs": "rimraf .libp2p"
  },
  "watch": {
    "build": {
      "patterns": "./src/**/*",
      "extensions": [
        ".ts",
        ".tsx"
      ]
    },
    "postbuild": "./dist/**/*"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@libp2p/logger": "^6.0.0",
    "@multiformats/multiaddr": "^12.5.1",
    "@types/compression": "^1.8.1",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.0",
    "@types/serve-static": "^2.0.0",
    "aegir": "^47.0.24",
    "axios": "^1.13.2",
    "core-js": "^3.46.0",
    "eslint": "^10.0.0",
    "execa": "^9.6.0",
    "globals": "^17.0.0",
    "mocha": "^11.7.5",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "typescript-transform-paths": "^3.5.5"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.2",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.0",
    "@helia/block-brokers": "^4.2.4",
    "@libp2p/autonat": "^3.0.0",
    "@libp2p/bootstrap": "^12.0.0",
    "@libp2p/circuit-relay-v2": "^4.0.0",
    "@libp2p/crypto": "^5.1.13",
    "@libp2p/dcutr": "^3.0.0",
    "@libp2p/identify": "^4.0.0",
    "@libp2p/interface": "^3.0.0",
    "@libp2p/peer-id": "^6.0.0",
    "@libp2p/ping": "^3.0.0",
    "@libp2p/pubsub-peer-discovery": "^12.0.0",
    "@libp2p/tcp": "^11.0.0",
    "@libp2p/webrtc": "^6.0.0",
    "@libp2p/websockets": "^10.0.0",
    "@libp2p/webtransport": "^6.0.0",
    "blockstore-core": "^5.0.4",
    "blockstore-level": "^2.0.5",
    "compression": "^1.8.1",
    "datastore-fs": "^10.0.6",
    "events": "^3.3.0",
    "express": "^5.1.0",
    "genversion": "^3.2.0",
    "libp2p": "^3.0.0",
    "p-queue": "^9.0.0",
    "rimraf": "^6.1.0",
    "serve-static": "^2.2.0",
    "uint8arrays": "^5.1.0",
    "uuid": "^13.0.0",
    "wherearewe": "^2.0.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@ipshipyard/node-datachannel",
      "classic-level",
      "core-js",
      "esbuild",
      "node-datachannel"
    ]
  }
}
