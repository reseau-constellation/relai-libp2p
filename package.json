    "@types/file-saver": "^2.0.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.1",
    "@types/serve-static": "^1.15.7",
    "@types/uuid": "^10.0.0",
    "core-js": "^3.40.0",
    "eslint": "^9.20.0",
    "execa": "^9.5.2",
    "globals": "^15.14.0",
    "mocha": "^10.8.2",
    "prettier": "^3.5.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.23.0",
    "typescript-transform-paths": "^3.5.3"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.0",
    "@chainsafe/libp2p-noise": "^16.0.1",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@helia/block-brokers": "^4.0.3",
    "@libp2p/autonat": "^2.0.19",
    "@libp2p/bootstrap": "^11.0.20",
    "@libp2p/circuit-relay-v2": "^3.1.10",
    "@libp2p/crypto": "^5.0.11",
    "@libp2p/dcutr": "^2.0.18",
    "@libp2p/identify": "^3.0.18",
    "@libp2p/interface": "^2.5.0",
    "@libp2p/peer-id": "^5.0.12",
    "@libp2p/pubsub-peer-discovery": "^11.0.1",
    "@libp2p/tcp": "^10.0.18",
    "@libp2p/webrtc": "^5.0.26",
    "@libp2p/websockets": "^9.1.5",
    "@libp2p/webtransport": "^5.0.25",
    "blockstore-core": "^5.0.2",
    "blockstore-level": "^2.0.1",
    "compression": "^1.7.5",
    "datastore-fs": "^10.0.2",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "genversion": "^3.2.0",
    "libp2p": "^2.6.0",
    "rimraf": "^6.0.1",
    "serve-static": "^1.16.2",
    "uint8arrays": "^5.1.0",
    "uuid": "^11.0.5",
    "wherearewe": "^2.0.1"
